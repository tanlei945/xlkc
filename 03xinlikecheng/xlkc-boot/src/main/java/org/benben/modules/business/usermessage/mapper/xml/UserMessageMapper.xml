<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.benben.modules.business.usermessage.mapper.UserMessageMapper">

    <select id="queryByLike" resultType="org.benben.modules.business.usermessage.vo.UserMessageVo">
          SELECT m.*,u.nickname from `user` u INNER JOIN user_message m on m.user1_id = u.id where m.state = 0
    </select>
</mapper>